public class MyHutoolDemo1 {

	public static void main(String[] args) {
		File file = FileUtil.file("a.txt");
		System.out.println(file);
		
		File file2 = FileUtil.file(".newFile","newFile2","a.txt");
		
		File touch = FileUtil.touch(file);
		

	}

}
